<aura:component controller="TemplatesComponentController" access="global">
    <aura:attribute name="showWarningMessage" type="Boolean" default="false"/>
    <aura:attribute name="reasonString" type="String" />
    <aura:attribute name="isValuesPresent" type="Boolean" default="false"/>

    <aura:attribute name="isPerformance" type="Boolean" />
    <aura:attribute name="activeSections" type="List" />
    <aura:attribute name="switchData" type="Object" />
    <aura:attribute name="toggleSpinner" type="Boolean" default="true" />
    <aura:attribute name="toggleSpinnerObject" type="Object" />
    <aura:attribute name="optionsInterval" type="List" default="Monthly,Quarterly,Half - yearly,Yearly" />
    <aura:attribute name="selectedIntervalVerifyEmployer" type="String" default="Monthly" />
    <aura:attribute name="selectedIntervalPhoneInsights" type="String" default="Monthly" />
    <aura:attribute name="openSelectPhoneModal" type="Boolean" default="false" />
    <aura:attribute name="serviceToggled" type="String" default="" />
    <aura:attribute name="openConfirmModal" type="Boolean" default="false" />
    <aura:attribute name="validationObject" type="Object" />
    <aura:attribute name="tempValueObject" type="Object" />
    <aura:attribute name="sameValue" type="Object" />
    <aura:attribute name="LimitsSelector" type="Boolean" />
    <aura:attribute name="showLimitsBox" type="Boolean" default="true" />
    <aura:attribute name="hyperLinkModal" type="Boolean" default="false" />
    <aura:attribute name="leadStageModal" type="Boolean" default="false" />
    <aura:attribute name="OppStageModal" type="Boolean" default="false" />
    <aura:attribute name="leadStageModalButton" type="Boolean" default="false" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="sfdcClassic" type="Boolean" default="false" />

    <aura:attribute name="availableStages" type="List" />
    <aura:attribute name="selectedStages" type="List" />
    <aura:attribute name="alreadySelectedStages" type="List" />
    <aura:attribute name="alreadySelectedStagesDemo" type="List" />

    <aura:attribute name="availableOppStages" type="List" />
    <aura:attribute name="alreadySelectedOppStages" type="List" />
    <aura:attribute name="selectedOppStages" type="List" />
    <aura:attribute name="alreadySelectedOppStagesDemo" type="List" />
    <aura:attribute name="showDynamicCriteriaModal" type="Boolean" default="false" />
    <aura:attribute name="ruleSets" type="List" />
    <aura:attribute name="selectedRuleset" type="String" />
    <!-- <aura:attribute name="selectedRulesetName" type="String" /> -->
    <aura:attribute name="editRuleset" type="Boolean" />
    <aura:attribute name="newRuleset" type="Boolean" />
    <aura:handler name="getNewRulesetData" event="c:createNewRulesetEvent" action="{!c.handleNewRulesetEvent}" />
    <!-- <aura:handler name="getNewRulesetData" event="c:createNewRulesetEvent" action="{!c.handleNewRulesetEvent}"/> -->
    <aura:attribute name="showAnalyzeModal" type="Boolean" />
    <aura:attribute name="showDeleteModal" type="Boolean" default="false" />

    <aura:attribute name="activeRulesetSections" type="List" default="['A','C']" />
    <aura:attribute name="activeRulesetSectionsMessage" type="String" default="" />

    <aura:attribute name="showMaintenanceDynamicCriteriaModal" type="Boolean" default="false" />

    <aura:attribute name="radioOptions" type="List"
        default="[{'label': 'Lead', 'value': 'Lead'},{'label': 'Contact', 'value': 'Contact'}]" />
    <aura:attribute name="selectedObject" type="String" default="Lead" />

    <aura:attribute name="mostActiveUserListContact" type="List" />
    <aura:attribute name="mostActiveUserListLead" type="List" />
    <aura:attribute name="showContactUserList" type="boolean" default="false" />
    <aura:attribute name="leadCount" type="String" />
    <aura:attribute name="contactCount" type="String" />
    <aura:attribute name="spinner" type="Boolean" default="false" />
    <aura:attribute name="analyzeSpinner" type="Boolean" default="false" />
    <aura:attribute name="showAlternateTextLead" type="Boolean" default="false" />
    <aura:attribute name="showAlternateTextContact" type="Boolean" default="false" />
    <aura:attribute name="showSaveOption" type="Boolean" default="false" />
    <aura:attribute name="automationTrigger" type="String" />
    <aura:attribute name="anticipatedQueriesCalculation" type="String" />
    <aura:attribute name="proactiveStatus" type="Boolean" default="false" />
    <aura:attribute name="opportunityRequestLimit" type="Integer" />

    <aura:attribute name="loadSpinner" type="boolean" default="false" />
    <aura:attribute name="configurationToast" type="boolean" default="false" />
    <aura:attribute name="configurationErrorToast" type="boolean" default="false" />
    <aura:attribute name="toastMessage" type="string" />
    <aura:method name="callChildJobAuto" action="{!c.scrollDiv}" />
    <aura:attribute name="topSection" type="Boolean" default="true" />
    <aura:attribute name="middleSection" type="Boolean" default="false" />
    <aura:attribute name="bottomSection" type="Boolean" default="false" />
    <aura:attribute name="variant" type="String" default="non-linear" />
    <aura:attribute name="hideUpdateButton" type="Boolean" default="false" />
    <aura:attribute name="MaitainQueueSalesEngagemet" type="Boolean" default="false" />

    <aura:if isTrue="{! v.spinner }">


        <aura:if isTrue="{!v.openConfirmModal}">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <!--lightning:buttonIcon iconName="utility:close"
                                        onclick="{! c.closeAbortModal }"
                                        alternativeText="close"
                                        variant="bare-inverse"
                                        class="slds-modal__close"/-->

                        <button class="
                                    slds-button slds-button_icon
                                    slds-modal__close
                                    slds-button_icon-inverse
                                    " title="Close" onclick="{!c.closeConfirmModal}">
                            <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                                size="small" class="confirmModalClose" />
                            <!--lightning-icon icon-name="utility:close"
                            alternative-text="close"
                            variant="inverse"
                            size="small" ></lightning-icon-->
                            <span class="slds-assistive-text">Close</span>
                        </button>

                        <!-- <div class="logo">
                            <img src="{!$Resource.ModigieLogo}" />
                        </div> -->
                        <div class="headerDiv">
                            <!--lightning:icon iconName="utility:warning" alternativeText="Warning!" class="headerDivIcon"  /-->
                            <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                                Are You Sure ?
                            </h2>
                        </div>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="
                                slds-modal__content
                                slds-p-around_medium
                                slds-is-relative
                                confirmModalBody
                                " id="modal-content-id-1">
                        Enabling this feature puts Leads and Contacts that have undergone
                        Hygiene into a data maintenance schedule for keeping them current and
                        accurate, tracking any changes in employment.
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer">
                        <lightning:button variant="destructive" label="Confirm" title="Confirm"
                            onclick="{! c.handleConfirmSubmit}" class="modalButtonMargin" />

                        <lightning:button variant="neutral" label="Cancel" title="Cancel"
                            onclick="{!c.closeConfirmModal}" class="modalButtonMargin" />
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>

        <aura:if isTrue="{!v.openSelectPhoneModal}">
            <c:SelectPhoneNumberForInsightsComp method="{!c.closeModal}"></c:SelectPhoneNumberForInsightsComp>
        </aura:if>

        <aura:if isTrue="{!v.configurationToast}">
            <div class="demo-only configurationToastDiv">
                <div class="slds-notify_container slds-is-relative">
                    <div class="slds-notify slds-notify_toast slds-theme_success" role="status">
                        <span class="slds-assistive-text">success</span>
                        <div class="slds-grid slds-gutters configurationToastSubDiv">
                            <span class="
                                        slds-icon_container
                                        slds-icon-utility-success
                                        slds-m-right_small
                                        slds-no-flex slds-align-top
                                        configurationToastSpan
                                        ">
                                <lightning:icon iconName="utility:success" alternativeText="Success!" variant="Success"
                                    size="small" />
                            </span>
                            <div class="slds-notify__content">
                                <h2 class="slds-text-heading_small">{!v.toastMessage}</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if>

        <aura:if isTrue="{!v.configurationErrorToast}">
            <div class="demo-only configurationErrorToastDiv">
                <div class="slds-notify_container slds-is-relative">
                    <div class="slds-notify slds-notify_toast slds-theme_error" role="status">
                        <span class="slds-assistive-text">error</span>
                        <div class="slds-grid slds-gutters configurationErrorToastSubDiv">
                            <span class="
                                        slds-icon_container
                                        slds-icon-utility-error
                                        slds-m-right_small
                                        slds-no-flex slds-align-top
                                        configurationErrorToastSpan
                                        ">
                                <lightning:icon iconName="utility:error" alternativeText="Error!" variant="Error"
                                    size="small" />
                            </span>
                            <div class="slds-notify__content">
                                <h2 class="slds-text-heading_small">{!v.toastMessage}</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if>

        <aura:if isTrue="{!v.hyperLinkModal}">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
                aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <button class="
                                    slds-button slds-button_icon
                                    slds-modal__close
                                    slds-button_icon-inverse
                                    " title="Close">
                            <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                                size="small" title="Close" onclick="{!c.closeHyperLinkModal}" />
                            <span class="slds-assistive-text">Close</span>
                        </button>
                        <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">
                            Job Automation Reference
                        </h2>
                    </header>
                    <div class="slds-modal__content slds-p-horizontal_x-large slds-p-vertical_medium textDiv"
                        id="modal-content-id-1">
                        <p>Admin will be able to configure multiple Use Cases for Modigie automation to trigger.</p>
                    </div>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>

        <aura:if isTrue="{!v.leadStageModal}">
            <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_small"
                aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1">
                <div class="slds- modal__container">
                    <header class="slds-modal__header">
                        <button class="
                                    slds-button slds-button_icon
                                    slds-modal__close
                                    slds-button_icon-inverse
                                    " title="Close">
                            <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                                size="small" title="Close" onclick="{!c.closeLeadStagesModal}" />
                        </button>
                        <h2 class="slds-modal__title slds-hyphenate">Select Lead Status</h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium slds-is-relative">
                        <div class="slds-grid slds-gutters">
                            <div class="slds-col slds-size_2-of-12"></div>
                            <div class="slds-col slds-size_8-of-12">
                                <aura:if isTrue="{!v.loadSpinner}">
                                    <div class="slds-spinner_container modalSpinner">
                                        <div class="slds-spinner--brand slds-spinner slds-spinner_medium" role="alert">
                                            <span class="slds-assistive-text">Loading</span>
                                            <div class="slds-spinner__dot-a"></div>
                                            <div class="slds-spinner__dot-b"></div>
                                        </div>
                                    </div>
                                </aura:if>

                                <lightning:dualListbox name="leadStages" label="Select Status"
                                    sourceLabel="Available Status" selectedLabel="Selected Status"
                                    fieldLevelHelp="Select Your Preferred Status" options="{!v.availableStages}"
                                    value="{!v.alreadySelectedStages}" onchange="{! c.handleStageChange }" />
                            </div>
                            <div class="slds-col slds-size_2-of-12"></div>
                        </div>
                    </div>
                    <footer class="slds-modal__footer">
                        <button class="slds-button slds-button_brand" onclick="{!c.doStageSave}">
                            Save
                        </button>
                        <button class="slds-button slds-button_neutral" onclick="{!c.closeLeadStagesModal}">
                            Cancel
                        </button>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>

        <aura:if isTrue="{!v.OppStageModal}">
            <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_small"
                aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1">
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <button class="
                                    slds-button slds-button_icon
                                    slds-modal__close
                                    slds-button_icon-inverse
                                    " title="Close">
                            <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                                size="small" title="Close" onclick="{!c.closeOppStagesModal}" />
                        </button>
                        <h2 class="slds-modal__title slds-hyphenate">
                            Select Opportunity Stages
                        </h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium slds-is-relative">
                        <lightning:select name="LimitsSelection" aura:id="Limits_No_Limits_Selection"
                            value="{!v.switchData.Opportunity_Limits_No_Limits_Selection}" class="select">
                            <option text="Limits" value="Limits" />
                            <option text="No Limits" value="No Limits" />
                        </lightning:select>
                        <aura:if isTrue="{!v.switchData.Opportunity_Limits_No_Limits_Selection == 'Limits'}">
                            <lightning:input label="Number of Requests per day" type="number" name="limit"
                                class="oppLimits" value="{!v.switchData.Opportunity_Request_Limit}" required="true" />
                        </aura:if>
                        <div class="slds-grid slds-gutters">
                            <div class="slds-col slds-size_2-of-12"></div>
                            <div class="slds-col slds-size_8-of-12">
                                <aura:if isTrue="{!v.loadSpinner}">
                                    <div class="slds-spinner_container modalSpinner">
                                        <div class="slds-spinner--brand slds-spinner slds-spinner_medium" role="alert">
                                            <span class="slds-assistive-text">Loading</span>
                                            <div class="slds-spinner__dot-a"></div>
                                            <div class="slds-spinner__dot-b"></div>
                                        </div>
                                    </div>
                                </aura:if>

                                <lightning:dualListbox name="oppStages" label="Select Stages"
                                    sourceLabel="Available Stages" selectedLabel="Selected Stages"
                                    fieldLevelHelp="Select Your Preferred Stage" options="{!v.availableOppStages}"
                                    value="{!v.alreadySelectedOppStages}" onchange="{! c.handleOppStageChange }" />
                            </div>
                            <div class="slds-col slds-size_2-of-12"></div>
                        </div>
                    </div>
                    <footer class="slds-modal__footer">
                        <button class="slds-button slds-button_brand" onclick="{!c.doOppStageSave}">
                            Save
                        </button>
                        <button class="slds-button slds-button_neutral" onclick="{!c.closeOppStagesModal}">
                            Cancel
                        </button>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>

        <div class="slds-page-header pageHeader">
            <div class="slds-page-header__row">
                <div class="slds-page-header__col-title">
                    <div class="slds-media">
                        <div class="slds-media__figure">
                            <span class="slds-icon_container">
                                <aura:if isTrue="{!v.isPerformance}" >
                                    <lightning:icon iconName="standard:marketing_actions" alternativeText="Event" />
                                    <aura:set attribute="else" >
                                        <lightning:icon iconName="standard:settings" alternativeText="Event" />
                                    </aura:set>
                                </aura:if>

                                <span class="slds-assistive-text">Authorization</span>
                            </span>
                        </div>
                        <div class="slds-media__body">
                            <div class="slds-page-header__name">
                                <div class="slds-page-header__name-title">
                                    <h1>
                                        <span class="slds-page-header__title slds-truncate">
                                            <aura:if isTrue="{!v.isPerformance}">
                                                Performance Automation
                                                <aura:set attribute="else">
                                                    Job Automation
                                                </aura:set>
                                            </aura:if>
                                        </span>
                                    </h1>
                                </div>
                            </div>
                            <a onclick="{!c.hyperLink}">Click Here for Overview</a>
                        </div>
                    </div>
                </div>

                <!-- <lightning:select
                                name="LimitsSelection"
                                aura:id="Limits_No_Limits_Selection"
                                value="{!v.switchData.Limits_No_Limits_Selection}"
                                onchange="{!c.handleChange}"
                                class="select"
                                >
                    <option text="Limits" value="Limits" />
                    <option text="No Limits" value="No Limits" />
                </lightning:select> -->
            </div>
        </div>

        <div class="slds-path slds-m-bottom_large">
            <!-- <div class="slds-grid slds-path__track">
                <div class="slds-grid slds-path__scroller-container">
                    <div class="slds-path__scroller">
                        <div class="slds-path__scroller_inner">
                            <ul class="slds-path__nav" role="listbox" aria-orientation="horizontal">
                                <li class="slds-path__item slds-is-active" role="presentation" aura:id="top">
                                    <a aria-selected="true" class="slds-path__link" href="#top" id="path-1"
                                        role="option" tabindex="0" onclick="{!c.clickTop}">
                                        <span class="slds-path__stage">
                                            <span class="slds-assistive-text">Current Stage:</span>
                                        </span>
                                        <span class="slds-path__title">Automated Enrichment</span>
                                    </a>
                                </li>
                                <li
                                    class="slds-path__item slds-is-incomplete"
                                    role="presentation"
                                    aura:id="middle"
                                    >
                                    <a
                                    aria-selected="false"
                                    class="slds-path__link"
                                    href="#middle"
                                    role="option"
                                    tabindex="-1"
                                    onclick="{!c.clickMiddle}"
                                    >
                                        <span class="slds-path__stage"> </span>
                                        <span class="slds-path__title"
                                            >Existing Records that require hygiene and enrichment</span
                                            >
                                    </a>
                                </li>
                                    <li class="slds-path__item slds-is-incomplete" role="presentation" aura:id="bottom">
                                        <a aria-selected="false" class="slds-path__link" href="#bottom" id="path-3"
                                            role="option" tabindex="-1" onclick="{!c.clickBottom}">
                                            <span class="slds-path__stage"> </span>
                                            <span class="slds-path__title">Employer Verification &amp; Data
                                                Maintenance</span>
                                        </a>
                                    </li>
                            </ul>
                        </div> 
                    </div>
                </div>
            </div> -->
            <aura:if isTrue="{!v.topSection}">
                <aura:if isTrue="{!and(v.showWarningMessage, v.isPerformance)}">
                    <div class="slds-notify slds-notify_alert banner" role="alert">
                        <span class="slds-assistive-text">Error</span>
                        <h2>You do not have permission for creating or updating ruleset, contact your CSM for access.
                        </h2>
                    </div>
                    <lightning:button class="createButtonDisabled" label="Create New Ruleset" iconName="utility:add"
                    iconPosition="left" variant="brand" onclick="{!c.openCreateNewRuleset}" disabled="true" />
                    <aura:set attribute="else">
                        <aura:if isTrue="{!v.isPerformance}">
                            <div class="slds-notify slds-notify_alert slds-alert_error" role="alert" style="background: green; border-radius: 0.25rem">
                                <span class="slds-assistive-text">Error</span>
                                <h2>{!v.reasonString}
                                </h2>
                            </div>
                        </aura:if>
                        
                        <!-- <div class="slds-scoped-notification slds-media slds-media_center slds-theme_success" role="status">
                            <div class="slds-media__body">
                                <lightning:icon iconName="standard:task2" alternativeText="Success!" variant="Success"
            title="success variant" />
                              Success! Your entry has been duplicated.
                              
                            </div>
                          </div> -->
                        <lightning:button class="createButton" label="Create New Ruleset" iconName="utility:add"
                    iconPosition="left" variant="brand" onclick="{!c.openCreateNewRuleset}" />
                    </aura:set>
                </aura:if>
                
            </aura:if>
        </div>

        <ui:scrollerWrapper class="scrollerSize">
            <div class="slds-m-around_small textDiv">
                <aura:if isTrue="{!v.topSection}">

                    <div class="slds-modal__content accordion" id="modal-content-id-1">

                        <lightning:accordion allowMultipleSectionsOpen="true"
                            onsectiontoggle="{! c.handleRulesetSectionToggle }"
                            activeSectionName="{! v.activeRulesetSections }">
                            <!-- <lightning:accordionSection label="Default Ruleset">
                                <aura:set attribute="actions">
                                    <lightning:buttonMenu aura:id="menu" alternativeText="Show menu" menuAlignment="right" iconSize="x-small">
                                        <lightning:menuItem value="New" label="Edit" />
                                    </lightning:buttonMenu>
                                </aura:set>
                                <aura:set attribute="body">
                                    Description of Default Ruleset
                                </aura:set>
                            </lightning:accordionSection> -->

                            <aura:iteration items="{!v.ruleSets}" var="ruleset" indexVar="index">
                                <lightning:accordionSection
                                    label="{!ruleset.title + ' - ' + (ruleset.active ? 'Active': 'Not Active')}">
                                    <aura:set attribute="actions">
                                        <!-- <div class="rulesetCheckbox">
                                            <label class="slds-checkbox_toggle slds-grid">
                                                <input type="checkbox" name="checkbox-toggle-18" value="checkbox-toggle-2" aria-describedby="checkbox-toggle-18" checked="" />
                                                <span id="checkbox-toggle-18" class="slds-checkbox_faux_container" aria-live="assertive">
                                                <span class="slds-checkbox_faux"></span>
                                                <span class="slds-checkbox_on">Enabled</span>
                                                <span class="slds-checkbox_off">Disabled</span>
                                                </span>
                                            </label>
                                        </div> -->
                                        <lightning:buttonMenu aura:id="menu" alternativeText="Show menu"
                                            menuAlignment="auto" iconSize="small" onselect="{!c.handleButtonMenu}">
                                            <aura:if isTrue="{!and(v.showWarningMessage, v.isPerformance)}">
                                                <lightning:menuItem value="{!ruleset.edit}" label="View"
                                                prefixIconName="utility:preview" />
                                                <lightning:menuItem value="{!ruleset.analyse}" label="Analyze"
                                                prefixIconName="utility:graph" />
                                                <aura:set attribute="else">
                                                    <aura:if isTrue="{!ruleset.active}">
                                                        <lightning:menuItem value="{!ruleset.status}" label="Disable"
                                                            prefixIconName="utility:close" />
                                                        <aura:set attribute="else">
                                                            <lightning:menuItem value="{!ruleset.status}" label="Enable"
                                                                prefixIconName="utility:success" />
                                                        </aura:set>
                                                    </aura:if>


                                                    <lightning:menuItem value="{!ruleset.edit}" label="Edit"
                                                        prefixIconName="utility:edit" />
                                                    <lightning:menuItem value="{!ruleset.delete}" label="Delete"
                                                        prefixIconName="utility:delete" />
