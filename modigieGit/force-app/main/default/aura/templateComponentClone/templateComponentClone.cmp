    <aura:attribute name="variant" type="String" default="non-linear" />
    <aura:attribute name="hideUpdateButton" type="Boolean" default="false" />




    <aura:if isTrue="{!v.openConfirmModal}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
            aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <!- Modal/Popup Box Header Starts here->
                <header class="slds-modal__header">
                    <!-lightning:buttonIcon iconName="utility:close"
                                      onclick="{! c.closeAbortModal }"
                                      alternativeText="close"
                                      variant="bare-inverse"
                                      class="slds-modal__close"/->

                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse"
                        title="Close" onclick="{!c.closeConfirmModal}">
                        <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                            size="small" class="confirmModalClose" />
                        <!-lightning-icon icon-name="utility:close"
                        alternative-text="close"
                        variant="inverse"
                        size="small" ></lightning-icon->
                        <span class="slds-assistive-text">Close</span>
                    </button>

                    <!- <div class="logo">
                        <img src="{!$Resource.ModigieLogo}" />
                    </div> ->
                    <div class="headerDiv">
                        <lightning:icon iconName="utility:warning" alternativeText="Warning!" class="headerDivIcon"  />
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate"
                        >Warning</h2>
                    </div>
                </header>
                <!-Modal/Popup Box Body Starts here->
                <div class="slds-modal__content slds-p-around_medium slds-is-relative confirmModalBody" id="modal-content-id-1"
                     >
                    This automation will run against the entire database, we strongly recommend you to go into process
                    builder and create additional filters to maximize the efficiency of this automation.
                </div>
                <!-Modal/Popup Box Footer Starts here->
                <footer class="slds-modal__footer">
                    <lightning:button variant="destructive" label="Confirm" title="Confirm"
                        onclick="{! c.handleConfirmSubmit}" class="modalButtonMargin" />

                    <lightning:button variant="neutral" label="Cancel" title="Cancel" onclick="{!c.closeConfirmModal}"
                        class="modalButtonMargin" />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>

    <aura:if isTrue="{!v.openSelectPhoneModal}">
        <c:SelectPhoneNumberForInsightsComp method="{!c.closeModal}"></c:SelectPhoneNumberForInsightsComp>
    </aura:if>


    <aura:if isTrue="{!v.configurationToast}">
        <div class="demo-only configurationToastDiv" >
            <div class="slds-notify_container slds-is-relative">
                <div class="slds-notify slds-notify_toast slds-theme_success" role="status">
                    <span class="slds-assistive-text">success</span>
                    <div class="slds-grid slds-gutters configurationToastSubDiv">
                        <span
                            class="slds-icon_container slds-icon-utility-success slds-m-right_small slds-no-flex slds-align-top configurationToastSpan"
                            title="Description of icon when needed">
                            <lightning:icon iconName="utility:success" alternativeText="Success!" variant="Success"
                                title="success variant small" size="small" />
                        </span>
                        <div class="slds-notify__content">
                            <h2 class="slds-text-heading_small ">{!v.toastMessage}</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </aura:if>

    <aura:if isTrue="{!v.configurationErrorToast}">
        <div class="demo-only configurationErrorToastDiv" >
            <div class="slds-notify_container slds-is-relative">
                <div class="slds-notify slds-notify_toast slds-theme_error" role="status">
                    <span class="slds-assistive-text">error</span>
                    <div class="slds-grid slds-gutters configurationErrorToastSubDiv" >
                        <span
                            class="slds-icon_container slds-icon-utility-error slds-m-right_small slds-no-flex slds-align-top configurationErrorToastSpan"
                            title="Description of icon when needed">
                            <lightning:icon iconName="utility:error" alternativeText="Error!" variant="Error"
                                title="error variant small" size="small" />
                        </span>
                        <div class="slds-notify__content">
                            <h2 class="slds-text-heading_small ">{!v.toastMessage}</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </aura:if>

    <aura:if isTrue="{!v.hyperLinkModal}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
            aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse"
                        title="Close">
                        <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                            size="small" title="Close" onclick="{!c.closeHyperLinkModal}" />
                        <span class="slds-assistive-text">Close</span>
                    </button>
                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">MODIGIE SERVICES Reference</h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium textDiv" id="modal-content-id-1"
                    >
                    <p>Modigie provides multiple discrete functional elements (services) in the managed package; version
                        (1.x). They are available using the user interface on the Lead, Lead List View, Contact, Contact
                        List View, and Campaign (if enabled) using buttons, dialog boxes and verifications (toast
                        messages). <span class="pSpan">Modigie services are also exposed through Salesforce
                            native automation using
                            invocable methods and process builder.</span> A user can access any combination of the
                        exposed services
                        using ad hoc and automation, stand-alone and in conjunction with one another.</p><br></br>

                    <br></br>
                    <p> <i>Get Modigie</i> - A service that takes first name, last name, company email and company name
                        and
                        returns
                        all information available from all Modigie services*:</p>

                    <ul class="slds-m-left_large textDivUl">
                        <li>Get Mobile Phone</li>
                        <li>Phone Insights</li>
                        <li>Verify Employer</li>
                        <li>Get LinkedIn URL </li>

                    </ul>

                    <p>*See a description of each API below.</p>

                    


                    <br></br>
                    <p><i>Phone Insights</i> - A Modigie service that takes an existing phone number, from a variety of
                        standard and custom fields, and returns:</p>

                    <ul class="slds-m-left_large textDivUl">
                        <li>Line type, Landline or Mobile</li>
                        <li>If Mobile</li>
                        <li>Best time to call</li>
                        <li>Line activity</li>

                    </ul>

                    <br></br>
                    <p> <i>Verify Employer</i> - A Modigie service that takes first name, last name, company email and
                        company
                        name and returns (if available):</p>

                    <ul class="slds-m-left_large textDivUl">
                        <li>Current employer</li>
                        <li>Current title</li>


                    </ul>

                    <br></br>
                    <p> <i>Get LinkedIn</i> - A Modigie service that takes first name, last name, company email and
                        company
                        name and returns (if available):</p>

                    <ul class="slds-m-left_large textDivUl">
                        <li>LinkedIn URL</li>

                    </ul>





                </div>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>

    <aura:if isTrue="{!v.leadStageModal}">
        <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_small"
            aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse"
                        title="Close">
                        <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                            size="small" title="Close" onclick="{!c.closeLeadStagesModal}" />
                    </button>
                    <h2 class="slds-modal__title slds-hyphenate">Select Lead Status</h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium slds-is-relative">

                    <div class="slds-grid slds-gutters">
                        <div class="slds-col slds-size_2-of-12">
                        </div>
                        <div class="slds-col slds-size_8-of-12">

                            <aura:if isTrue="{!v.loadSpinner}">
                                <div class="slds-spinner_container modalSpinner" >
                                    <div class="slds-spinner-brand  slds-spinner slds-spinner_medium" role="alert">
                                        <span class="slds-assistive-text">Loading</span>
                                        <div class="slds-spinner__dot-a"></div>
                                        <div class="slds-spinner__dot-b"></div>
                                    </div>
                                </div>
                            </aura:if>

                            <lightning:dualListbox name="leadStages" label="Select Status"
                                sourceLabel="Available Status" selectedLabel="Selected Status"
                                fieldLevelHelp="Select Your Preferred Status" options="{!v.availableStages}"
                                value="{!v.alreadySelectedStages}" onchange="{! c.handleStageChange }" />
                        </div>
                        <div class="slds-col slds-size_2-of-12">
                        </div>
                    </div>


                </div>
                <footer class="slds-modal__footer">
                    <button class="slds-button slds-button_brand" onclick="{!c.doStageSave}">Save</button>
                    <button class="slds-button slds-button_neutral" onclick="{!c.closeLeadStagesModal}">Cancel</button>

                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>




    <aura:if isTrue="{!v.OppStageModal}">
        <section role="dialog" tabindex="-1" class="slds-modal slds-fade-in-open slds-modal_small"
            aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse"
                        title="Close">
                        <lightning:icon iconName="utility:close" alternativeText="Connected" variant="inverse"
                            size="small" title="Close" onclick="{!c.closeOppStagesModal}" />
                    </button>
                    <h2 class="slds-modal__title slds-hyphenate">Select Opportunity Stages</h2>
                </header>
                <div class="slds-modal__content slds-p-around_medium slds-is-relative">

                    <div class="slds-grid slds-gutters">
                        <div class="slds-col slds-size_2-of-12">
                        </div>
                        <div class="slds-col slds-size_8-of-12">

                            <aura:if isTrue="{!v.loadSpinner}">
                                <div class="slds-spinner_container modalSpinner" >
                                    <div class="slds-spinner-brand  slds-spinner slds-spinner_medium" role="alert">
                                        <span class="slds-assistive-text">Loading</span>
                                        <div class="slds-spinner__dot-a"></div>
                                        <div class="slds-spinner__dot-b"></div>
                                    </div>
                                </div>
                            </aura:if>

                            <lightning:dualListbox name="oppStages" label="Select Stages" sourceLabel="Available Stages"
                                selectedLabel="Selected Stages" fieldLevelHelp="Select Your Preferred Stage"
                                options="{!v.availableOppStages}" value="{!v.alreadySelectedOppStages}"
                                onchange="{! c.handleOppStageChange }" />
                        </div>
                        <div class="slds-col slds-size_2-of-12">
                        </div>
                    </div>


                </div>
                <footer class="slds-modal__footer">
                    <button class="slds-button slds-button_brand" onclick="{!c.doOppStageSave}">Save</button>
                    <button class="slds-button slds-button_neutral" onclick="{!c.closeOppStagesModal}">Cancel</button>

                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>




    <div class="slds-page-header pageHeader">
        <div class="slds-page-header__row">
            <div class="slds-page-header__col-title">
                <div class="slds-media">
                    <div class="slds-media__figure">
                        <span class="slds-icon_container " title="opportunity">
                            <lightning:icon iconName="standard:settings" alternativeText="Event" title="Event" />

                            <span class="slds-assistive-text">Authorization</span>
                        </span>

                    </div>
                    <div class="slds-media__body">
                        <div class="slds-page-header__name">
                            <div class="slds-page-header__name-title">
                                <h1>
                                    <span class="slds-page-header__title slds-truncate">Job
                                        Automation</span>

                                </h1>

                            </div>

                        </div>
                        <a onclick="{!c.hyperLink}">More Information</a>
                    </div>


                </div>

            </div>


            <lightning:select name="LimitsSelection" aura:id="Limits_No_Limits_Selection"
                value="{!v.switchData.Limits_No_Limits_Selection}" onchange="{!c.handleChange}" class="select">
                <option text="Limits" value="Limits" />
                <option text="No Limits" value="No Limits" />
            </lightning:select>

        </div>
    </div>

    <div class="slds-path slds-m-bottom_large">
        <div class="slds-grid slds-path__track">
            <div class="slds-grid slds-path__scroller-container">
                <div class="slds-path__scroller">
                    <div class="slds-path__scroller_inner">
                        <ul class="slds-path__nav" role="listbox" aria-orientation="horizontal">
                            <li class="slds-path__item  slds-is-active" role="presentation" aura:id="top">
                                <a aria-selected="true" class="slds-path__link" href="#top" id="path-1" role="option"
                                    tabindex="0" onclick="{!c.clickTop}">
                                    <span class="slds-path__stage">

                                        <span class="slds-assistive-text">Current Stage:</span>
                                    </span>
                                    <span class="slds-path__title">New Lead or Contact</span>
                                </a>
                            </li>
                            <li class="slds-path__item slds-is-incomplete" role="presentation" aura:id="middle">
                                <a aria-selected="false" class="slds-path__link" href="#middle" role="option"
                                    tabindex="-1" onclick="{!c.clickMiddle}">
                                    <span class="slds-path__stage">

                                    </span>
                                    <span class="slds-path__title">Existing Records that require hygiene and
                                        enrichment</span>
                                </a>
                            </li>
                            <li class="slds-path__item slds-is-incomplete" role="presentation" aura:id="bottom">
                                <a aria-selected="false" class="slds-path__link" href="#bottom" id="path-3"
                                    role="option" tabindex="-1" onclick="{!c.clickBottom}">
                                    <span class="slds-path__stage">

                                    </span>
                                    <span class="slds-path__title">Existing Records that require data maintenance</span>
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>

        </div>




    </div>

    <ui:scrollerWrapper class="scrollerSize">

        <div class="slds-m-around_small textDiv ">



            <aura:if isTrue="{!v.topSection}">
                <h2 id="top">Modigie considers automation to be highly relevant to all our clients. In an effort to
                    create a simplified model to support automation across the largest client landscape, Modigie
                    has created three key areas of automation that most every sales and marketing organization
                    will require:</h2><br></br>

                <ul>
                    <li>Top of the funnel = New Lead or Contact</li>
                    <li>Middle of the funnel = Existing targets that require hygiene and enrichment</li>
                    <li>Bottom of the funnel = Existing targets that require data maintenance</li>
                </ul>

                <h2>Modigie has exposed invocable methods in the Salesforce Managed Package. These
                    methods allow
                    customers to leverage pre-built automation found in the Modigie Configuration Page, custom
                    process builder automation comation to crereated by customers, and APEX autate highly
                    effective automation schemes to perform Enrichment, Hygiene and Data Maintenance of contact
                    information. Each Modigie service discussed in the Modigie Service Reference is available as
                    pre-built automation and an invocable method. Modigie has provided a selection of
                    pre-built automation that can be used as cloned and customized or used as a template for
                    completely custom automation using Salesforce Process Builder and custom APEX.
                </h2><br></br><br></br>


                <h2><b>Customized Settings for you</b> (using Process Builder, no custom code needed)</h2>
                <h2> Contact Modigie directly to discuss developing customized rules, settings for you that
                    match your unique business requirements.</h2><br></br>




                <!- top of the funnel ->

                <div>
                    <h1 class="funnelTop"><b>&nbsp;Top of the funnel (Hygiene &amp; Enrichment): </b></h1>

                    <ol>
                        <li>
                            <h2><b>Upon creation of a new lead or contact</b> (including Marketing
                                Qualified Leads<b>(MQLs)</b>):</h2>
                        </li>

                        <article class="slds-m-around_small slds-card olDiv">

                            <div class="slds-card__header slds-grid ">
                                <header class="slds-media slds-media_center slds-has-flexi-truncate ">
                                    <div class="slds-media__figure">
                                        <span class="slds-icon_container">
                                            <lightning:icon iconName="utility:right" alternativeText="Connected"
                                                size="xx-small" />
                                            <span class="slds-assistive-text">Get Modigie</span>

                                        </span>

                                    </div>

                                    <div class="slds-text-heading_medium slds-media__body textDiv">
                                        <h2>Get Modigie</h2>
                                    </div>



                                    <div class="slds-no-flex">
                                        <div class="slds-form-element">
                                            <aura:if
                                                isTrue="{!or(v.toggleSpinner, v.toggleSpinnerObject.No_Mobile_Information)}">
                                                <div class="slds-spinner_container modalSpinner"
                                                    >
                                                    <div class="slds-spinner-brand  slds-spinner slds-spinner_small"
                                                        role="alert">
                                                        <span class="slds-assistive-text">Loading</span>
                                                        <div class="slds-spinner__dot-a"></div>
                                                        <div class="slds-spinner__dot-b"></div>
                                                    </div>
                                                </div>
                                            </aura:if>
                                            <div class="slds-grid slds-gutters">




                                                <aura:if
                                                    isTrue="{!and(v.switchData.No_Mobile_Information,v.showLimitsBox)}">
                                                    <div class="slds-form-element slds-col"
                                                        aura:id="NumberOfLimitsField">

                                                        <label class="slds-form-element__label" for="text-input-id-1">
                                                            <abbr class="slds-required" title="required">* </abbr>Number
                                                            of
                                                            Requests per
                                                            day</label>
                                                        <div class="slds-form-element__control">
                                                            <input type="number" id="text-input-id-1"
                                                                placeholder="Enter limit…" required=""
                                                                aria-describedby="error-message-unique-id"
                                                                class="slds-input" aria-invalid="true" min="1"
                                                                autocomplete="off"
                                                                value="{!v.switchData.Get_Modigie_Invocable_Limit}"
                                                                oninput="{!c.handleLimitChange}"
                                                                data-name="Get_Modigie_Invocable_Limit" />
                                                        </div>

                                                        <aura:if isTrue="{!v.switchData.No_Mobile_Information}">
                                                            <div class="c-container">
                                                                <aura:if
                                                                    isTrue="{!and(!or(v.validationObject.Get_Modigie_Invocable_Limit, v.sameValue.Get_Modigie_Invocable_Limit),v.showLimitsBox)}">
                                                                    <lightning:layout horizontalAlign="space">
                                                                        <lightning:layoutItem padding="around-small">
                                                                            <div class="noMobileInfoSave"
                                                                                >
                                                                                
                                                                                <button
                                                                                    class="slds-button slds-button_brand noMobileInfoSaveButton"
                                                                                    type="button" title="Brand action"
                                                                                    
                                                                                    onclick="{!c.confirmLimit}"
                                                                                    data-name="Get_Modigie_Invocable_Limit">Save</button>
                                                                            </div>
                                                                            
                                                                        </lightning:layoutItem>
                                                                        <lightning:layoutItem padding="around-small">
                                                                            <div class="noMobileInfoCancel">
                                                                                
                                                                                <button
                                                                                    class="slds-button slds-button_brand noMobileInfoCancelButton"
                                                                                    type="button" title="Brand action"
                                                                                     
                                                                                    onclick="{!c.displayExistingLimit}"
                                                                                    data-name="Get_Modigie_Invocable_Limit">Cancel</button>
                                                                            </div>
                                                                            
                                                                        </lightning:layoutItem>
                                                                    </lightning:layout>
                                                                </aura:if>
                                                                <aura:if
                                                                    isTrue="{!and(v.validationObject.Get_Modigie_Invocable_Limit,v.showLimitsBox)}">
                                                                    <div class="slds-form-element__help validValueError"
                                                                        id="error-message-unique-id"
                                                                        >Enter a
                                                                        valid
                                                                        value.</div>
                                                                </aura:if>
                                                            </div>
                                                        </aura:if>
                                                    </div>
                                                </aura:if>

                                                <div class="slds-col togglePosition">
                                                    <label class="slds-checkbox_toggle slds-grid">
                                                        <!-input type="checkbox" name="checkbox-toggle-18" value="checkbox-toggle-18" aria-describedby="checkbox-toggle-18" /->
                                                        <ui:inputCheckbox aura:id="No_Mobile_Information"
                                                            class="slds-input" change="{!c.handleToggle}"
                                                            value="{!v.switchData.No_Mobile_Information}" />
                                                        <span id="checkbox-toggle-18"
                                                            class="slds-checkbox_faux_container" aria-live="assertive">
                                                            <aura:if
